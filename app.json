{
  "expo": {
    "name": "Motorcycle Manager",
    "slug": "motorcycle-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.victornievesmedeiros136.motorcyclemanager"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-notifications"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98ed43ef-f250-4714-af16-5069569db9d8"
      }
    },
    "owner": "victornievesmedeiros136"
  }
}
